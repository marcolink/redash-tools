{
  "version": "0.2.1",
  "name": "redash-js-client",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "repository": "marcolink/redash-tools",
  "homepage": "https://github.com/marcolink/redash-tools/packages/client",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --tsconfig tsconfig.build.json --verbose --noClean",
    "build": "tsdx build --tsconfig tsconfig.build.json",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch --verbose",
    "test:ci": "tsdx test --coverage",
    "lint": "tsdx lint",
    "prepare": "yarn run build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "author": "Marco Link",
  "module": "dist/redash-client.esm.js",
  "size-limit": [
    {
      "path": "dist/redash-client.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/redash-client.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.8",
    "@types/jest-when": "^3.5.0",
    "husky": "^6.0.0",
    "jest-when": "^3.2.1",
    "mockdate": "^3.0.5",
    "size-limit": "^7.0.8",
    "tsdx": "^0.14.1",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "dayjs": "^1.10.5",
    "listr": "^0.14.3",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7"
  }
}
